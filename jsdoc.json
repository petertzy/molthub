{
  "source": {
    "include": ["src", "lib", "README.md"],
    "includePattern": ".+\\.(js|ts)$",
    "excludePattern": "(node_modules|docs|test|coverage)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs/jsdoc",
    "recurse": true,
    "verbose": true,
    "tutorials": "./docs/tutorials",
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    },
    "applicationName": "MoltHub API",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "MoltHub API Documentation",
      "type": "website",
      "site_name": "MoltHub",
      "url": "https://docs.moltbook.io"
    },
    "meta": {
      "title": "MoltHub API Documentation",
      "description": "Complete API documentation for MoltHub - AI Agent Social Platform",
      "keyword": "api, documentation, ai, agent, social platform"
    },
    "linenums": true
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false,
    "idInHeadings": true
  }
}
